<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserActivity"
    android:background="?attr/colorOnSecondary">

    <ImageView
        android:layout_width="209dp"
        android:layout_height="208dp"
        android:layout_marginEnd="244dp"
        android:layout_marginBottom="624dp"
        android:alpha=".70"
        android:background="@drawable/circle_shape"
        android:cropToPadding="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="199dp"
        android:layout_height="188dp"
        android:layout_marginStart="292dp"
        android:alpha=".75"
        android:background="@drawable/circle_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.738"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="180dp"
        android:layout_height="169dp"
        android:layout_marginTop="632dp"
        android:layout_marginEnd="264dp"
        android:alpha=".73"
        android:background="@drawable/circle_shape"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />


    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@mipmap/profile"
        app:layout_constraintBottom_toTopOf="@+id/firstName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="ContentDescription" />

    <EditText
        android:id="@+id/firstName"
        android:layout_width="272dp"
        android:layout_height="55dp"
        android:layout_marginBottom="8dp"
        android:ems="11"
        android:hint="First Name"
        android:inputType="textPersonName"
        android:textColorHint="@color/orange"
        android:textColor="?attr/colorOnPrimary"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="Autofill,HardcodedText" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="272dp"
        android:layout_height="55dp"
        android:ems="11"
        android:hint="Last Name"
        android:inputType="textPersonName"
        android:textColor="?attr/colorOnPrimary"
        android:textColorHint="@color/orange"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334"
        tools:ignore="Autofill,HardcodedText" />

    <EditText
        android:id="@+id/editTextAge"
        android:layout_width="272dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:ems="11"
        android:hint="Age"
        android:inputType="number"
        android:textColor="?attr/colorOnPrimary"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        tools:ignore="Autofill,HardcodedText" />


    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="272dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:ems="11"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:hint="Birthday"
        android:inputType="date"
        android:textColor="?attr/colorOnPrimary"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextAge"
        tools:ignore="Autofill,HardcodedText" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="272dp"
        android:layout_height="93dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="99dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:text="Male"
            android:textSize="18sp"
            android:elegantTextHeight="true"
            android:textColor="?attr/colorOnPrimary"
            tools:ignore="HardcodedText,RtlHardcoded" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:textSize="18sp"
            android:elegantTextHeight="true"
            android:text="Female"
            android:textColor="?attr/colorOnPrimary"
            tools:ignore="HardcodedText,RtlHardcoded" />
    </RadioGroup>

    <EditText
        android:id="@+id/editTextWeight"
        android:layout_width="272dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:ems="11"
        android:hint="Weight"
        android:inputType="number"
        android:textColorHint="@color/orange"
        android:textColor="?attr/colorOnPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup"
        tools:ignore="Autofill,HardcodedText" />

    <TextView
        android:id="@+id/textView99"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:gravity="center_vertical|end"
        android:paddingStart="6dp"
        android:layout_marginBottom="2dp"
        android:paddingEnd="11dp"
        android:text="kg"
        android:textColor="?attr/colorOnPrimary"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextWeight"
        app:layout_constraintEnd_toEndOf="@+id/editTextWeight"
        app:layout_constraintTop_toTopOf="@+id/editTextWeight" />

    <EditText
        android:id="@+id/editTextHeight"
        android:layout_width="272dp"
        android:layout_height="52dp"
        android:layout_marginTop="8dp"
        android:ems="11"
        android:hint="Height"
        android:inputType="number"
        android:textColor="?attr/colorOnPrimary"
        android:textColorHint="@color/orange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextWeight"
        tools:ignore="Autofill,HardcodedText" />

    <TextView
        android:id="@+id/textView69"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:gravity="center_vertical|end"
        android:paddingStart="6dp"
        android:paddingEnd="10dp"
        android:layout_marginBottom="3dp"
        android:text="cm"
        android:textColor="?attr/colorOnPrimary"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextHeight"
        app:layout_constraintEnd_toEndOf="@+id/editTextHeight"
        app:layout_constraintTop_toTopOf="@+id/editTextHeight" />

    <Button
        android:id="@+id/nextBtn"
        android:layout_width="140dp"
        android:layout_height="55dp"
        android:layout_marginBottom="72dp"
        app:cornerRadius="18dp"
        android:elegantTextHeight="true"
        android:text="NEXT"
        android:textSize="20sp"
        android:alpha=".9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,UsingOnClickInXml" />

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:max="100"
        android:layout_marginTop="24dp"
        android:paddingLeft="28dp"
        android:paddingRight="28dp"
        android:progress="25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="115dp"
        android:layout_height="25dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="36dp"
        android:text="Gender:"
        android:textColor="@color/orange"
        android:textSize="19sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextDate"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>